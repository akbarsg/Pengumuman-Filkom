<?xml version='1.0' encoding='utf-8'?>
<widget id="com.faathin.filkom" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Filkom Apps</name>
    <description>
        Jangan ngaku anak filkom kalau belum install aplikasi ini.
    </description>
    <author email="ahmad.riyadh.al@faathin.com" href="http://faathin.com">
        Filkom Apps Dev
    </author>
    <content src="index.html" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
